# JPEG codec information
#
[codec]
layout=6
version=1.3.4.1
priority=HIGHEST
name=JPEG
description=Joint Photographic Experts Group
magic-numbers=FF D8
extensions=jpg;jpeg;jpe;jif;jfif;jfi
mime-types=image/jpeg

[read-features]
features=STATIC;META-DATA@CODEC_INFO_FEATURE_ICCP@
tuning=jpeg-dct-method;jpeg-optimize-coding;jpeg-smoothing-factor

[write-features]
features=STATIC;META-DATA@CODEC_INFO_FEATURE_ICCP@
tuning=
pixel-formats=BPP8-GRAYSCALE;@SAIL_JPEG_CODEC_INFO_WRITE_EXT@BPP24-YCBCR;BPP32-CMYK;BPP32-YCCK
properties=
compression-types=JPEG
default-compression=JPEG
compression-level-min=0
compression-level-max=100
compression-level-default=15
compression-level-step=1
