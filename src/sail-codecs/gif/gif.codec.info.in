# GIF codec information
#
[codec]
layout=4
version=1.3.1
name=GIF
description=Graphics Interchange Format
magic-numbers=47 49 46 38 37 61;47 49 46 38 39 61
extensions=gif
mime-types=image/gif

[read-features]
features=STATIC;ANIMATED;META-DATA
output-pixel-formats=BPP32-RGBA;BPP32-BGRA
default-output-pixel-format=@SAIL_DEFAULT_READ_OUTPUT_PIXEL_FORMAT@

[write-features]
features=STATIC;ANIMATED;META-DATA
properties=
interlaced-passes=4
compression-types=LZW
default-compression=LZW
compression-level-min=0
compression-level-max=0
compression-level-default=0
compression-level-step=0

[write-pixel-formats-mapping]
BPP32-RGBA=SOURCE
