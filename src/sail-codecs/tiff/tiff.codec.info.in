# TIFF codec information
#
[codec]
layout=6
version=1.0.1
name=TIFF
description=Tagged Image File Format
magic-numbers=49 49 2A 00;4D 4D 00 2A
extensions=tif;tiff
mime-types=image/tiff;image/tiff-fx

[read-features]
features=STATIC;MULTI-PAGED;META-DATA;ICCP

[write-features]
features=STATIC;MULTI-PAGED;META-DATA;ICCP
output-pixel-formats=BPP32-RGBA
properties=
compression-types=@SAIL_TIFF_CODEC_INFO_COMPRESSIONS@
default-compression=@SAIL_TIFF_CODEC_INFO_DEFAULT_COMPRESSION@
compression-level-min=0
compression-level-max=0
compression-level-default=0
compression-level-step=0
